@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Peliculas varias</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>


<ul>
    @foreach (var item in Model.MovieList) //Para tener acceso a la propiedad(MovieList) necesito llamar al modelo de la pagina(Model)
    {
        <li>@item.Name</li>
    }
</ul>